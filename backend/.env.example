# Example environment variables for StockMaster (copy to .env and edit)
# For Postgres (recommended for hackathon):
# DATABASE_URL=postgresql+psycopg2://<user>:<password>@<host>:<port>/<dbname>

# For quick local SQLite dev, you can use:
# DATABASE_URL=sqlite:///./dev_stockmaster.db

# JWT secret for signing tokens (set a strong random value in production)
STOCKMASTER_SECRET=replace-with-a-secure-random-string

# Token expiry in minutes
ACCESS_TOKEN_EXPIRE_MINUTES=60
